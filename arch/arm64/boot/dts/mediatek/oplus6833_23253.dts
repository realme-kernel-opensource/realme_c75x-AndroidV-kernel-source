&dsi0 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	panel9@0 {
		compatible = "oplus23053,truly,td4160,dphy,vdo";
		reg = <0>;
		reset-gpios = <&pio 86 0>;
		ldo-gpios = <&pio 41 0>;
		bias-gpios = <&pio 137 0>,
				<&pio 138 0>;
		pinctrl-names = "default";
