	};

	panel10@1 {
		compatible = "oplus23053,boe,ili9883c,dphy,vdo";
		reg = <1>;
		reset-gpios = <&pio 86 0>;
		ldo-gpios = <&pio 41 0>;
		bias-gpios = <&pio 137 0>,
			<&pio 138 0>;
		port {
			panel_in10: endpoint {
		    		remote-endpoint = <&dsi_out>;
	    		};
		};
	};
	ports {
		port {
			dsi_out: endpoint {
				remote-endpoint = <&panel_in10>;
			};
		};
	};
};
